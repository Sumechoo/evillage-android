<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="nl.worth.clangnotifications">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application>
        <service
                android:name=".firebase.ClangIntentService"
                android:exported="false">
        </service>

        <activity android:name=".ui.ClangActivity">
        </activity>
        <activity android:name=".ui.ThankYouActivity">
        </activity>
        <activity android:name=".ui.CreateAccountActivity">
        </activity>

        <service
                android:name=".firebase.ClangFirebaseMessagingService"
                android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>